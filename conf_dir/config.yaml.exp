# Bot admins
admin:
  id: [123456, 789012]

# Database configuration
database:
  host: localhost
  port: 5432
  user: admin
  password: secret
  dbname: my_database

# Aliyun API configuration
aliyun:
  appcode: your_app_code

# Short URL service configuration
shorturl:
  api: https://api.example.com

# ICP API configuration
icp:
  url: https://icp-api.example.com/query/web

xiatou:
  id: [123456, 789012]
  openai_api: https://api.openai.com
  openai_api_key: sk-abcdefghijklmnopqrstuvwxyz1234567890
  openai_api_model: gpt-3.5-turbo

translate:
  token: 1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
  location: global

inline:
  empty_placeholder_image: https://pbs.twimg.com/media/HAckcxubgAARphg?format=jpg&name=4096x4096

# Custom Bot API server configuration
botapi:
  enable: false

# OCR plugin configuration (Aliyun Qwen OCR)
ocr:
  # Required: Model Studio API Key (DashScope API Key)
  api_key: sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
  # OpenAI compatible endpoint
  base_url: https://dashscope.aliyuncs.com/compatible-mode/v1
  # Model name (e.g. qwen-vl-ocr, qwen-vl-ocr-latest)
  model: qwen-vl-ocr-latest
  # Fallback prompt when /ocr is used without custom prompt
  default_prompt: Please output only the text content from the image without any additional descriptions or formatting.
  # Request timeout in seconds
  timeout: 60
  # Optional image params
  min_pixels: 3072
  max_pixels: 8388608
  enable_rotate: false
